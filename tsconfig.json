{
    "extends": "astro/tsconfigs/strict",
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "src/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@content/*": [
                "src/content/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ]
        },
        "jsx": "react-jsx",
        "jsxImportSource": "react"
    }
}